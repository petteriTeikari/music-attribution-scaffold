{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "API Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(attribution_requests_total[5m])",
          "legendFormat": "{{method}} {{endpoint}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "title": "Request Latency (p50, p95, p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(agent_response_latency_seconds_bucket[5m]))",
          "legendFormat": "p50 {{tool}}"
        },
        {
          "expr": "histogram_quantile(0.95, rate(agent_response_latency_seconds_bucket[5m]))",
          "legendFormat": "p95 {{tool}}"
        },
        {
          "expr": "histogram_quantile(0.99, rate(agent_response_latency_seconds_bucket[5m]))",
          "legendFormat": "p99 {{tool}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Confidence Score Distribution",
      "type": "histogram",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "attribution_confidence_score_bucket",
          "legendFormat": "{{assurance_level}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        }
      }
    },
    {
      "title": "Drift Detection Events",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "sum(drift_detected_total) by (drift_type)",
          "legendFormat": "{{drift_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        }
      }
    },
    {
      "title": "Center Bias Detections",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 8 },
      "targets": [
        {
          "expr": "center_bias_detections_total",
          "legendFormat": "total"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      }
    }
  ],
  "refresh": "10s",
